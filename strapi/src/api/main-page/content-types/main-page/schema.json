{
  "kind": "singleType",
  "collectionName": "main_pages",
  "info": {
    "singularName": "main-page",
    "pluralName": "main-pages",
    "displayName": "Main page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "equipment": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::equipment-category.equipment-category"
    },
    "aboutCompany": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "services": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::page.page"
    },
    "videos": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "youtube-video.youtube-video",
      "required": true
    },
    "carousel": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "carousel.carousel"
    }
  }
}
